      <description>&lt;p&gt;&amp;lt;%*&#xA;function toSlug(title) {&#xA;return title&#xA;.toLowerCase()&#xA;.normalize(&amp;ldquo;NFD&amp;rdquo;)&#xA;.replace(/[\u0300-\u036f]/g, &amp;ldquo;&amp;rdquo;)&#xA;.replace(/[^a-z0-9\s-]/g, &amp;ldquo;&amp;rdquo;)&#xA;.trim()&#xA;.replace(/\s+/g, &amp;ldquo;-&amp;rdquo;);&#xA;}&lt;/p&gt;&#xA;&lt;h3 id=&#34;-&#34; class=&#34;scroll-mt-8 group&#34;&gt;&#xA;  let qcFileName = await tp.system.prompt(&amp;ldquo;Post Title&amp;rdquo;)&#xA;let titleName = toSlug(qcFileName);&#xA;await tp.file.rename (titleName);&#xA;await tp.file.move(&amp;quot;/notes/&amp;quot; + titleName &amp;ldquo;/index&amp;rdquo;);&#xA;await this.app.vault.createFolder(&amp;quot;/notes/&amp;quot; + titleName + &amp;ldquo;/images&amp;rdquo;);&#xA;-%&amp;gt;&#xA;  &#xA;    &lt;a href=&#34;#-&#34;&#xA;        class=&#34;no-underline hidden opacity-50 hover:opacity-100 !text-inherit group-hover:inline-block&#34;&#xA;        aria-hidden=&#34;true&#34; title=&#34;Link to this heading&#34; tabindex=&#34;-1&#34;&gt;&#xA;        &lt;svg&#xA;  xmlns=&#34;http://www.w3.org/2000/svg&#34;&#xA;  width=&#34;16&#34;&#xA;  height=&#34;16&#34;&#xA;  fill=&#34;none&#34;&#xA;  stroke=&#34;currentColor&#34;&#xA;  stroke-linecap=&#34;round&#34;&#xA;  stroke-linejoin=&#34;round&#34;&#xA;  stroke-width=&#34;2&#34;&#xA;  class=&#34;lucide lucide-link w-4 h-4 block&#34;&#xA;  viewBox=&#34;0 0 24 24&#34;&#xA;&gt;&#xA;  &lt;path d=&#34;M10 13a5 5 0 0 0 7.54.54l3-3a5 5 0 0 0-7.07-7.07l-1.72 1.71&#34; /&gt;&#xA;  &lt;path d=&#34;M14 11a5 5 0 0 0-7.54-.54l-3 3a5 5 0 0 0 7.07 7.07l1.71-1.71&#34; /&gt;&#xA;&lt;/svg&gt;&#xA;&#xA;    &lt;/a&gt;&#xA;  &#xA;&lt;/h3&gt;&#xA;&lt;h3 id=&#34;tags-&#34; class=&#34;scroll-mt-8 group&#34;&gt;&#xA;  title: &amp;lt;% qcFileName %&amp;gt;&#xA;date: &amp;lt;% tp.file.creation_date(&amp;ldquo;YYYY-MM-DDTHH:mm:ssZ&amp;rdquo;) %&amp;gt;&#xA;draft: false&#xA;tags: []&#xA;  &#xA;    &lt;a href=&#34;#tags-&#34;&#xA;        class=&#34;no-underline hidden opacity